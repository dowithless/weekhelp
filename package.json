{
  "name": "weekhelp",
  "displayName": "Weekhelp",
  "description": "自动收集整理 git 提交信息，为写周报提供素材。",
  "version": "0.0.5",
  "repository": "https://github.com/dowithless/weekhelp",
  "publisher": "dowithless",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "weekhelp.openWeekhelpFolder",
        "title": "Weekhelp: Open Weekhelp Folder"
      },
      {
        "command": "weekhelp.openWeekhelpFile",
        "title": "Weekhelp: Open Weekhelp File"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Week Help",
      "properties": {
        "weekhelp.folderPath": {
          "type": "string",
          "default": "",
          "description": "The weekhelp folder path."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dayjs": "^1.11.11"
  }
}
